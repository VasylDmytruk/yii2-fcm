{
    "name": "autoxloo/yii2-fcm",
    "description": "Yii2 wrap on autoxloo/fcm",
    "type": "yii2-extension",
    "keywords": [
        "yii2",
        "fcm",
        "firebase cloud messaging"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Vasyl Dmytruk",
            "email": "vasyl.dmytruk@xloo.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.0",
        "yiisoft/yii2": "~2.0.0"
    },
    "require-dev": {
        "symfony/var-dumper": "*"
    },
    "autoload": {
        "psr-4": {
            "autoxloo\\yii2\\fcm\\": "src"
        }
    }
}
